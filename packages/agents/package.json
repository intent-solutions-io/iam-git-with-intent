{
  "name": "@gwi/agents",
  "version": "0.8.0",
  "description": "Agent implementations for Git With Intent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./triage": {
      "types": "./dist/triage/index.d.ts",
      "import": "./dist/triage/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.js"
    },
    "./reviewer": {
      "types": "./dist/reviewer/index.d.ts",
      "import": "./dist/reviewer/index.js"
    },
    "./infra": {
      "types": "./dist/infra/index.d.ts",
      "import": "./dist/infra/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --passWithNoTests",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@gwi/core": "*",
    "firebase-admin": "12.7.0",
    "vite": "7.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
