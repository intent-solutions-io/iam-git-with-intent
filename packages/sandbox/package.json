{
  "name": "@gwi/sandbox",
  "version": "0.10.0",
  "description": "Sandbox execution layer for Git With Intent: Docker, KVM, and Deno isolates",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    },
    "./export": {
      "types": "./dist/export/index.d.ts",
      "import": "./dist/export/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --passWithNoTests",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@gwi/core": "*",
    "dockerode": "^4.0.4",
    "ssh2": "^1.16.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.31",
    "@types/node": "^20.10.0",
    "@types/ssh2": "^1.15.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
