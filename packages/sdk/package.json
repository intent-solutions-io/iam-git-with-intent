{
  "name": "@gwi/sdk",
  "version": "0.7.0",
  "description": "TypeScript SDK client for the Git With Intent API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "generate:types": "npx tsx ../../scripts/generate-sdk-types.ts",
    "validate:types": "npx tsx ../../scripts/validate-sdk-types.ts",
    "prebuild": "npm run generate:types",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --passWithNoTests",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "git-with-intent",
    "gwi",
    "sdk",
    "typescript",
    "api-client",
    "git",
    "devops",
    "automation"
  ],
  "author": "Intent Solutions <jeremy@intentsolutions.io>",
  "license": "MIT",
  "dependencies": {
    "@gwi/core": "*",
    "firebase-admin": "12.7.0",
    "vite": "7.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
