{
  "name": "@gwi/engine",
  "version": "0.8.0",
  "description": "Git With Intent - Agent execution engine with hook system",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@gwi/agents": "*",
    "@gwi/core": "*",
    "@gwi/integrations": "*",
    "firebase-admin": "12.7.0",
    "vite": "7.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "git",
    "intent",
    "ai",
    "agent",
    "engine",
    "hooks"
  ],
  "author": "Intent Solutions <jeremy@intentsolutions.io>",
  "license": "MIT"
}
