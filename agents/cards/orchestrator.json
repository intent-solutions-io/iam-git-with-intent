{
  "$schema": "https://a2a.dev/schemas/agent-card-v1.json",
  "id": "spiffe://intent.solutions/agent/gwi/orchestrator",
  "name": "GWI Orchestrator",
  "description": "Central coordinator for Git With Intent. Routes work between agents, manages workflows, handles escalations.",
  "version": "0.1.0",
  "capabilities": [
    "workflow-management",
    "agent-routing",
    "escalation-handling",
    "health-monitoring"
  ],
  "endpoints": {
    "a2a": "/a2a/orchestrator",
    "health": "/health"
  },
  "models": {
    "default": "gemini-2.0-flash",
    "routing": "gemini-2.0-flash"
  },
  "workflows": [
    {
      "id": "pr-resolve",
      "name": "PR Conflict Resolution",
      "steps": ["triage", "resolver", "reviewer"]
    },
    {
      "id": "issue-to-code",
      "name": "Issue to Code",
      "steps": ["triage", "coder", "reviewer", "test"]
    }
  ],
  "metadata": {
    "owner": "Intent Solutions",
    "contact": "jeremy@intentsolutions.io",
    "repository": "https://github.com/intent-solutions-io/git-with-intent"
  }
}
