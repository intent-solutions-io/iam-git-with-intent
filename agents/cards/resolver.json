{
  "$schema": "https://a2a.dev/schemas/agent-card-v1.json",
  "id": "spiffe://intent.solutions/agent/gwi/resolver",
  "name": "GWI Resolver Agent",
  "description": "Deep conflict resolution using Claude Sonnet/Opus. Analyzes merge conflicts and produces semantically correct resolutions.",
  "version": "0.1.0",
  "capabilities": [
    "conflict-resolution",
    "code-merge",
    "semantic-analysis",
    "strategy-selection"
  ],
  "endpoints": {
    "a2a": "/a2a/resolver",
    "health": "/health"
  },
  "models": {
    "default": "claude-sonnet-4-20250514",
    "complex": "claude-opus-4-20250514"
  },
  "strategies": [
    "merge-both",
    "accept-ours",
    "accept-theirs",
    "custom"
  ],
  "inputs": {
    "pr": "PRMetadata",
    "conflict": "ConflictInfo",
    "complexity": "ComplexityScore from Triage"
  },
  "outputs": {
    "resolvedContent": "string",
    "explanation": "string",
    "confidence": "0-100",
    "strategy": "merge-both|accept-ours|accept-theirs|custom"
  },
  "metadata": {
    "owner": "Intent Solutions",
    "contact": "jeremy@intentsolutions.io"
  }
}
