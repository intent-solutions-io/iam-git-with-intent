{
  "name": "@gwi/cli",
  "version": "0.1.0",
  "description": "CLI for Git With Intent - AI-powered DevOps automation",
  "type": "module",
  "bin": {
    "gwi": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@gwi/core": "*",
    "@gwi/agents": "*",
    "@gwi/integrations": "*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
